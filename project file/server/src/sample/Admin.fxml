<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="410.0" prefWidth="618.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AdminController">
   <children>

      <ListView fx:id="listView" layoutX="45.0" layoutY="67.0" prefHeight="200.0" prefWidth="379.0" />
      <Button fx:id="Browse" layoutX="45.0" layoutY="284.0" mnemonicParsing="false" onAction="#browseAction" text="Browse" />
      <Button fx:id="Add" layoutX="113.0" layoutY="284.0" mnemonicParsing="false" onAction="#addAction" text="Add File" />
      <Button fx:id="Remove" layoutX="188.0" layoutY="284.0" mnemonicParsing="false" onAction="#removeAction" text="Remove file" />
      <Button fx:id="logoutButton" layoutX="435.0" layoutY="348.0" mnemonicParsing="false" onAction="#logoutAction" text="Logout" />
      <Button fx:id="Next" layoutX="513.0" layoutY="348.0" mnemonicParsing="false" onAction="#nextAction" text="Next" />
      <Button fx:id="clientbutton" layoutX="47.0" layoutY="348.0" mnemonicParsing="false" onAction="#clientAction" text="Clients" />
      <ListView fx:id="sizeList" layoutX="435.0" layoutY="67.0" prefHeight="200.0" prefWidth="138.0" />
      <Label fx:id="fileName" layoutX="45.0" layoutY="38.0" prefHeight="17.0" prefWidth="186.0" />
      <Label fx:id="fileSize" layoutX="435.0" layoutY="38.0" prefHeight="17.0" prefWidth="98.0" />

   </children>
</AnchorPane>
